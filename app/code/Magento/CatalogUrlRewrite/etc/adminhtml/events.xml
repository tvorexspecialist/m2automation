<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="catalog_product_to_website_change">
        <observer name="catalog_product_to_website_change" instance="Magento\CatalogUrlRewrite\Observer\ProductToWebsiteChangeObserver"/>
    </event>
    <event name="controller_action_catalog_product_save_entity_after">
        <observer name="controller_notifications_on_duplicate_urls" instance="Magento\CatalogUrlRewrite\Observer\DuplicateUrlControllerAfterSaveObserver"/>
    </event>
    <event name="controller_action_catalog_category_save_entity_after">
        <observer name="controller_notifications_on_duplicate_urls" instance="Magento\CatalogUrlRewrite\Observer\DuplicateUrlControllerAfterSaveObserver"/>
    </event>
    <event name="controller_action_catalog_category_move_after">
        <observer name="controller_notifications_on_duplicate_urls" instance="Magento\CatalogUrlRewrite\Observer\DuplicateUrlControllerAfterSaveObserver"/>
    </event>
</config>
